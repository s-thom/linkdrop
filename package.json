{
  "name": "linkdrop-d071",
  "version": "1.8.1",
  "private": true,
  "description": "",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
    "docker": "docker compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@remix-run/css-bundle": "^2.11.2",
    "@remix-run/express": "^2.11.2",
    "@remix-run/node": "^2.11.2",
    "@remix-run/react": "^2.11.2",
    "@remix-run/server-runtime": "^2.11.2",
    "@remix-run/v1-route-convention": "^0.1.4",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-deep-equal": "^3.1.3",
    "lucide-react": "^0.439.0",
    "morgan": "^1.10.0",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@remix-run/dev": "^2.11.2",
    "@remix-run/eslint-config": "^2.11.2",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.14.2",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "happy-dom": "^15.7.3",
    "msw": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prisma": "^5.19.1",
    "start-server-and-test": "^2.0.6",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "overrides": {
    "@testing-library/cypress": {
      "cypress": "$cypress"
    },
    "msw": {
      "typescript": "$typescript"
    }
  }
}
